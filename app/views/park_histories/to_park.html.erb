<% if flash[:notice] %>
  <div style="color: red">
  	<% flash[:notice].each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </div>
<% end %>
<%= form_tag :action => "park" do  %>
	<%= label_tag "Plate Number" %>
	<%= text_field_tag :plate_number %>
	<%= submit_tag "park" %>
<% end %>
<br />
<%= form_tag :action => "find_ticket" do %>
	<%= label_tag "ticket ID" %>
	<%= text_field_tag :id %>
	<%= submit_tag "query" %>
<% end %>
<br />
<b><%= @count %></b> stalls left
<br />
<%= link_to "stalls", :controller => "stalls", :action => "stall_index"%>
